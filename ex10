package estrutura_decisao;
import java.util.Scanner;
public class ex10 {

public static void main(String[] args) {
// TODO Auto-generated method stub

// Entrar com dois valores quaisquer. Exibir o maior deles, se existir, caso
// contrário, enviar mensagem avisando que os números são idênticos.


Scanner ler = new Scanner(System.in);
     
       double num01, num02;
     
       System.out.printf("Digite o primeiro número: ");
       num01 = ler.nextDouble();
     
       System.out.printf("Digite o segundo número: ");
       num02 = ler.nextDouble();
                   
       if (num01 > num02) {
           System.out.printf("O primeiro número é o maior");
       }else if (num01 == num02) {
           System.out.printf("Números idênticos");
           
       }else {
               System.out.printf("O segundo número é o maior");

       }

}

}

